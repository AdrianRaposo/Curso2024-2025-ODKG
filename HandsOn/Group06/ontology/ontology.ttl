@prefix mair: <http://www.MadridAirQuality.es/air_quality#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


# Define Classes

ex:Provincia a owl:Class ;
    rdfs:label "Provincia" .

ex:Municipio a owl:Class ;
    rdfs:label "Municipio" .

ex:Estacion a owl:Class ;
    rdfs:label "Estacion" .

ex:Parametro a owl:Class ;
    rdfs:label "Parametro" .

ex:Observacion a owl:Class ;
    rdfs:label "Observacion" .


# Define Object Properties

ex:hasProvincia a owl:ObjectProperty ;
    rdfs:label "has provincia" ; 
    rdfs:domain ex:Municipio ; 
    rdfs:range ex:Provincia . 

ex:hasMunicipio a owl:ObjectProperty ;
    rdfs:label "has municipio" ; 
    rdfs:domain ex:Provincia ; 
    rdfs:range ex:Municipio . 

ex:hasEstacion a owl:ObjectProperty ;
    rdfs:label "has estacion" ; 
    rdfs:domain ex:Municipio ; 
    rdfs:range ex:Estacion . 

ex:monitors a owl:ObjectProperty ;
    rdfs:label "monitors" ; 
    rdfs:domain ex:Estacion ; 
    rdfs:range ex:Parametro . 

ex:recordsObservation a owl:ObjectProperty ;
    rdfs:label "records observation" ; 
    rdfs:domain ex:Estacion ; 
    rdfs:range ex:Observacion . 


# Define Datatype Properties

ex:hasFecha a owl:DatatypeProperty ;
    rdfs:label "has fecha" ; 
    rdfs:domain ex:Observacion ; 
    rdfs:range xsd:date .

ex:hasMagnitud a owl:DatatypeProperty ;
    rdfs:label "has magnitud" ; 
    rdfs:domain ex:Contaminante ; 
    rdfs:range xsd:string .

